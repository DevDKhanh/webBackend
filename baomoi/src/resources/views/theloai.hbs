<div class="main_category pt-5 pb-4">
    <div class="container">
        <div class="text-danger"><h3>{{title}}</h3></div>
        <div class="row">
            {{#each data}}
            <div class="col-sm-4 mt-3">
                <div class="card_category">
                    <a href="{{this.linknews}}">
                    <div class="img_card-category">
                        <img src="{{this.linkimg}}" alt="">
                    </div>
                    <div class="title_card-category">
                        {{this.namenews}}
                    </div>
                    </a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>